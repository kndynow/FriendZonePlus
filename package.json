{
    "name": "friendzoneplus",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently -n \"FRONTEND,BACKEND,WAIT\" -c \"blue,green,yellow\" -k \"npm run dev --prefix frontend\" \"npm run backend\"",
        "dev": "npm start",
        "frontend": "npm run dev --prefix frontend",
        "backend": "dotnet watch --project backend/src/FriendZonePlus.API/FriendZonePlus.API.csproj",
        "save": "node scripts/save-seed.js"
    },
    "devDependencies": {
        "concurrently": "^9.1.0"
    }
}